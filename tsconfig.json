{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@Hooks/*": ["src/Hooks/*"],
      "@Apps/*": ["src/Apps/*"],
      "@services/*": ["src/services/*"],
      "@utilities/*": ["src/utilities/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"],
    }
  }
}